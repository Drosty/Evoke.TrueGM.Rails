<h2>Salaries in System</h2>

<%= link_to "Upload new Data", new_admin_salary_path, class: "btn btn-success" %>
<%= link_to "Process Top 10 :: Week (" + @max_week.to_s + ")", admin_salaries_process_path, method: :post, class: "btn btn-info" %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Player Name</th>
      <th>Position</th>
      <th>Week</th>
      <th>Salary</th>
      <th>Projection</th>
    </tr>
  </thead>

  <tbody>
    <% @salaries.each do |salary_info| %>
      <tr>
        <td><%= salary_info.player_name %></td>
        <td><%= salary_info.position %></td>
        <td><%= salary_info.week %></td>
        <td><%= salary_info.salary %></td>
        <td><%= salary_info.projection %></td>
      </tr>
    <% end %>
  </tbody>
</table>
